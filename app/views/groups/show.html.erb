<div class = 'show_wrapper'> 
<h1 class = 'heading left'>List of the <span><%=h @group.name %></span> students </h1>	
<div class = 'right'<%= link_to image_tag("closelabel.gif", :title=> "close"), emails_url %> </div>
  <br /><br />
	 <table  class="student_list" >
		<tr>    <th>Roll No.</th>
			<th>Student name</th>
                        <th>Parent name </th>
                        <th>Mobile No.</th>
		</tr>
			<% for student in @students%>
		<tr class="<%=cycle("odd", "even")%>">
                        <td><%= student.roll_number%></td>
			<td><%= student.name%></td>
                        <td> <%= student.parent %> </td>
                        <td><%=h student.number %></td>
		</tr>
		<%end%>
 	</table>
</div>	    
