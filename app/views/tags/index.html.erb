<div id ="user-new"><%= link_to 'New tag', new_tag_path %></div>

<h1>Listing tags</h1>

<table class ="listing">
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>View</th>
  </tr>

<% @tags.each do |tag| %>
  <tr class="<%=cycle("odd", "even")%>">
    <td><%=h tag.name %></td>
    <td><%=h tag.description %></td>
    <td><%= link_to 'View', tag%></td>
  </tr>
<% end %>
</table>

<br />
  <div class="clear"></div>


